{
  "provider": "google",
  "localeConfig": {
    "source": "en-US",
    "target": ["zh-CN"]
  },
  "groups": [
    {
      "name": "app",
      "plugin": "json",
      "paths": [
        {
          "source": "./src/locales/en-US.json",
          "target": "./src/locales/{locale}.json"
        }
      ]
    }
  ]
}
