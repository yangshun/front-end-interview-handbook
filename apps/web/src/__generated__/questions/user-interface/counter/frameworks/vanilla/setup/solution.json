    "/src/index.js": {
      "code": "import './styles.css';\n\n(() => {\n  let count = 0;\n  const $countEl = document.querySelector('#count');\n\n  document\n    .querySelector('button')\n    .addEventListener('click', () => {\n      count++;\n      $countEl.textContent = count;\n    });\n})();\n"
