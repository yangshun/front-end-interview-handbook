{
  "name": "gfe",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=greatfrontend...",
    "dev": "turbo run dev --filter=greatfrontend...",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx}\"",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "turbo": "2.5.3"
  },
  "engines": {
    "node": ">= 22.15.0"
  },
  "packageManager": "pnpm@10.9.0"
}
