{
  "name": "gfe",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=greatfrontend...",
    "dev": "turbo run dev --filter=greatfrontend...",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx}\"",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "turbo": "latest"
  },
  "engines": {
    "node": "18.18.2",
    "pnpm": "8.15.8"
  },
  "packageManager": "pnpm@8.15.8"
}
